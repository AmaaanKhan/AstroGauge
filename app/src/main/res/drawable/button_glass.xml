<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="#60FFFFFF">
    <item>
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <gradient
                        android:startColor="#404A6E"
                        android:endColor="#2D3656"
                        android:angle="135" />
                    <corners android:radius="16dp" />
                </shape>
            </item>
            <item>
                <shape android:shape="rectangle">
                    <stroke
                        android:width="1dp"
                        android:color="#5A6B8C" />
                    <corners android:radius="16dp" />
                </shape>
            </item>
        </layer-list>
    </item>
</ripple>